"        background-color: lightyellow !important;\n"
"}\n"
"/* 保留 */\n"
"#ticket_list td.field3-4 {\n"
"        background-color: white !important;\n"
"}\n"
"/* 完了 */\n"
"#ticket_list td.field3-5 {\n"
"        background-color: lightcyan !important;\n"
"}\n"
